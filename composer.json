{
    "name": "mamba\/latipay",
    "description": "A php latipay SDK",
    "license": "MIT",
    "authors": [
        {
            "name": "lakely",
            "email": "abcde666111@126.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-openssl": "*",
        "ext-simplexml":"*",
        "ext-libxml": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/http-foundation": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Mamba\\Latipay\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "mockery/mockery": "^1.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mamba\\Latipay\\PayServiceProvider"
            ]
        }
    }
}
